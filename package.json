{
  "name": "fintech-core-engine",
  "version": "0.1.3",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "lint": "echo 'lint pending'",
    "migrate": "node scripts/migrate.mjs",
    "test": "jest",
    "test:cov": "jest --coverage",
    "check:concurrency": "node scripts/concurrency-double-spend-check.mjs"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.8",
    "@nestjs/core": "^10.4.8",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.4.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "js-yaml": "^4.1.1",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "ts-jest",
        {
          "diagnostics": false
        }
      ]
    },
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.8",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.1",
    "@types/passport": "^1.0.10",
    "@types/passport-jwt": "^3.0.8",
    "@types/pg": "^8.11.10",
    "@types/swagger-ui-express": "^4.1.8",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
